Algoritmo CalcularCocienteResto
	
	// Calcula el cociente y el resto de dos números introducidos por teclado.
	
	Definir dividendo, divisor, cociente, resto Como Real;
	
	Escribir "Introduce el dividendo:"
	Leer dividendo;
	
	Escribir "Introduce el divisor:"
	Leer divisor;
	
	cociente=dividendo/divisor;
	Escribir "El cociente es: " cociente;
	
	resto=dividendo%divisor;
	Escribir "El resto es: " resto;
	
	FinAlgoritmo
